{% extends 'base.html.twig' %}

{% block title %}AddFiles index{% endblock %}

{% block body %}
    <h1>AddFiles index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Src</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for add_file in add_files %}
            <tr>
                <td>{{ add_file.id }}</td>
                <td>{{ add_file.name }}</td>
                <td>{{ add_file.src }}</td>
                <td>
                    <a href="{{ path('app_add_files_show', {'id': add_file.id}) }}">show</a>
                    <a href="{{ path('app_add_files_edit', {'id': add_file.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_add_files_new') }}">Create new</a>
{% endblock %}
